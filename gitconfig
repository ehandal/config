[alias]
	b = branch
	c = commit
	co = checkout
	d = diff
	ds = diff --staged
	l = log --oneline
	s = status --short --branch
	up = pull --rebase
[diff]
	tool = vimdiff
[include]
	path = .gitconfig.local
[merge]
	conflictstyle = diff3
	tool = vimdiff
[push]
	default = simple
[url "git@github.com:"]
	pushInsteadOf = git://github.com/
	pushInsteadOf = https://github.com/
